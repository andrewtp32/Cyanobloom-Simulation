<?xml version="1.0"?>
<sdf version="1.5">
    <world name="default">
   
        <!-- A ground plane-->
        <include>
            <uri>model://ground_plane</uri>
            <pose>0 0 -0.75 0 0 0</pose>
            <size>50 50</size>
            <color>0 0 200</color>
        </include>

        <!-- A global light source -->
        <include>
            <uri>model://sun</uri>
        </include>

    </world>
</sdf>
